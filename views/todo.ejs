    <body>
        <h1>My Todo</h1>
        <div id="todo-table">
            <form method="POST">
                <input type="text" name="item" placeholder="Add new Item ..." required>
                <button type="submit">Add Item</button>
            </form>
            <ul>
                <% for(var i=0; i<data.length; i++){ %>
                    <li><%= data[i].item %></li>
                <% } %>
            </ul>
        </div>
    </body>
</html>
